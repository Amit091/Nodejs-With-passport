<h1 class="mt-4 breadcrumb-item">Dashboard</h1>
<%include ./partials/messages %>
    <p class="lead mb-3 "> <i class="fa fa-user-astronaut"></i>Welcome
        <em><strong><%= user._id %></strong></em>
    </p>
    <h1>Profile Account</h1>
    <div class="row">
        <div class="card">
            <div class="card-body">
                <p>
                    <%=user._id%>
                </p>
                <p>
                    <%=user.name%>
                </p>
                <p>
                    <%=user.email%>
                </p>
                <p>
                    <%=user.password%>
                </p>
            </div>
        </div>
    </div>
    <br>
    <a href="/users/logout" class="btn btn-secondary">Logout</a>